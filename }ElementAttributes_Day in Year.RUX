    , 'M' | SUBST(!Day in Year, 1, 2) | ' D' | SUBST(!Day in Year, 4, 2)
,IF (DB('}ElementAttributes_Day in Year', !Day in Year, 'El Type') @='D MTD'
    , 'M' | SUBST(!Day in Year, 1, 2) | ' D' | SUBST(!Day in Year, 4, 2) | ' MTD'
