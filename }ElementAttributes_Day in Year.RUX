    % DB('}ElementAttributes_Day in Year', !Day in Year, 'El Type') @='M FYTD'
    % DB('}ElementAttributes_Day in Year', !Day in Year, 'El Type') @='M FYTG'
    % DB('}ElementAttributes_Day in Year', !Day in Year, 'El Type') @='Q'
    , 'Q' | STR(['Quarter Index'],1,0)
    , CONTINUE
);


